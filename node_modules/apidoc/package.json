  },
  "devDependencies": {
    "apidoc-example": "*",
    "jshint": "^2.7.0",
    "mocha": "~2.2.4",
    "npm-check-updates": "^1.5.1",
    "path-to-regexp": "^1.0.3",
    "semver": "^4.3.4",
    "should": "~6.0.1"
